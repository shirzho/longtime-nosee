<link rel="stylesheet" type="text/css" href="css/index.css">
<table>
    <tr>
        <th>Card Name</th>
        <th>User Pair</th>
        <th>Card Content</th>
        
    </tr>
    <% if (obj.length) { %>
        <% for(var i = 0; i < obj.length; i++) { %>
            <% var livecard = obj[i]%>
            <tr>
                <td>
                    <%= livecard.cardName %>
                </td>

                <td>
                    <%= livecard.pair %>
                </td>
                <td>
                    <%= livecard.cardContent %>
                </td>
            </tr>
        <% }; %>
    <% } %>
</table>